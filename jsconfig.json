{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$ui/*": ["src/ui/*"],
      "$ui": ["src/ui"],
      "$style/*": ["src/style/*"],
      "$style": ["src/style"],
      "$store/*": ["src/store/*"],
      "$store": ["src/store"],
      "$components/*": ["src/components/*"],
      "$components": ["src/components"],
      "$lib/*": ["src/lib/*"],
      "$lib": ["src/lib"],
      "$utils/*": ["src/utils/*"],
      "$utils": ["src/utils"],
      "$pages/*": ["src/pages/*"],
      "$pages": ["src/pages"],
      "$layout/*": ["src/layout/*"],
      "$layout": ["src/layout"],
      "$hooks/*": ["src/hooks/*"],
      "$hooks": ["src/hooks"],
      "$data/*": ["src/data/*"],
      "$data": ["src/data"],
      "$assets/*": ["src/assets/*"],
      "$assets": ["src/assets"],
      "$src/*": ["src/*"],
      "$src": ["src"],
      "$/*": ["/*"]
    }
  },
  "exclude": ["node_modules", "build", "dist"]
}
